{
  "name": "@dprint/rust-printer",
  "version": "0.1.0",
  "description": "Wasm bindings for printing with rust.",
  "main": "dist/dprint-rust-printer.js",
  "types": "lib/dprint-rust-printer.d.ts",
  "author": "David Sherret",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsherret/dprint/issues"
  },
  "homepage": "https://github.com/dsherret/dprint#readme",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsherret/dprint.git"
  },
  "scripts": {
    "build:all": "rimraf dist && yarn build:rust && yarn rollup && yarn build:declarations",
    "build:rust": "wasm-pack build --target nodejs --out-dir src-ts/pkg",
    "build:declarations": "ts-node --project scripts/tsconfig.json --transpile-only scripts/buildDeclarations.ts",
    "rollup": "copyfiles -u 1 src-ts/pkg/**/* dist && rollup --config",
    "test:all": "yarn build:rust && cross-env TS_NODE_COMPILER=\"ttypescript\" TS_NODE_TRANSPILE_ONLY=\"true\" mocha --opts mocha.opts",
    "test:debug": "yarn test --inspect-brk"
  },
  "dependencies": {
    "@dprint/types": "~0.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/ts-nameof": "^3.2.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "mocha": "^6.2.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-typescript2": "^0.24.0",
    "ts-nameof": "^3.2.0",
    "ts-node": "^8.3.0",
    "ttypescript": "^1.5.7"
  }
}
