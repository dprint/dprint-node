  },
  "devDependencies": {
    "@types/ts-nameof": "^4.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-morph": "^7.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
